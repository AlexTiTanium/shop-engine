Ext.define('Core.applications.{{ name|ucfirst }}', {
  extend: 'Core.lib.Application',
  name: '{{ name|ucfirst }}',
  appFolder: '{{ name }}',
  allowMultiInstance: false,
  controllers: ['Main'],
  views: ['Main'],
  storage: [],
  model: [],

  launch: function (){
    var me = this;

    var window = me.getView('Main');
    window.show();
  }

});